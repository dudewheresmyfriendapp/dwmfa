<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.parse.starter"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="11"
        android:targetSdkVersion="19" />

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />

    <application
        android:name="com.parse.starter.ParseApplication"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name" >
        <activity
            android:name="com.parse.starter.ParseStarterProjectActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.parse.starter.SignUpOrLogInActivity"
            android:label="@string/app_name" >
        </activity>
        <activity
            android:name="com.parse.starter.LoginActivity"
            android:label="@string/title_activity_login" >
        </activity>
        <activity
            android:name="com.parse.starter.SignUpActivity"
            android:label="@string/app_name" >
        </activity>
        <activity
            android:name="com.parse.starter.DispatchActivity"
            android:label="@string/title_activity_dispatch" >
        </activity>
        <activity
            android:name="com.parse.starter.HomeScreenActivity"
            android:label="@string/home_screen_title" >
        </activity>
        <activity
            android:name="com.parse.starter.NewGroupActivity"
            android:label="@string/title_activity_new_group" >
        </activity>
        <activity
            android:name="com.parse.starter.NFCGroupActivity"
            android:label="@string/title_activity_nfcgroup" >
        </activity>
        <activity
            android:name="com.parse.starter.PostListActivity"
            android:label="@string/title_activity_post_list" >
        </activity>
        <activity
            android:name="com.parse.starter.NewUserPostActivity"
            android:label="@string/title_activity_new_user_post" >
        </activity>
    </application>

</manifest>